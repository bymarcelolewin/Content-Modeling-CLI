{
  "name": "content-modeling-cli",
  "version": "1.0.0",
  "description": "A CLI tool for building, templating, and managing content models for Contentful.",
  "bin": {
    "cmc": "./cli-tools/cmc.js"
  },
  "files": [
    "cli-assets",
    "cli-components",
