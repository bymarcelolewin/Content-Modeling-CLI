    "contentful-cli",
    "cli",
    "content-modeling",
    "headless-cms",
    "content-architecture"
  ],
  "author": {
    "name": "Marcelo Lewin",
    "email": "marcelo@intelligentcontentacademy.com",
    "url": "https://www.intelligentcontent.academy"
  },
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^13.1.0",
    "contentful-cli": "^3.8.8",
    "contentful-management": "^11.52.2",
    "fs-extra": "^11.3.0",
    "module-alias": "^2.2.3"
